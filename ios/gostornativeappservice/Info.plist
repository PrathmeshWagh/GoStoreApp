	<string>Allow Gostor to access your location</string>
	<key>NSLocationAlwaysUsageDescription</key>
	<string>Allow Gostor to access your location when in the background.</string>
	<key>NSCameraUsageDescription</key>
	<string>Allow Gostor to access your camera</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>Allow Gostor to access your microphone</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Allow Gostor to access your photo library</string>
	<key>NSSpeechRecognitionUsageDescription</key>
	<string>Allow Gostor to access sppech recognition</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
  	<string>Allow Gostor to access your location</string>
